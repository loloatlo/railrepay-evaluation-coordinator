{
  "name": "@railrepay/evaluation-coordinator",
  "version": "1.0.0",
  "description": "Multi-step evaluation workflow orchestration service for RailRepay",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-esm src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "migrate:up": "node-pg-migrate up --migrations-table evaluation_coordinator_pgmigrations",
    "migrate:down": "node-pg-migrate down",
    "migrate:create": "node-pg-migrate create",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@railrepay/kafka-client": "^1.0.0",
    "@railrepay/metrics-pusher": "^1.1.0",
    "@railrepay/postgres-client": "^1.0.0",
    "@railrepay/winston-logger": "^1.0.0",
    "axios": "^1.13.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-pg-migrate": "^7.0.0",
    "pg": "^8.11.3",
    "prom-client": "^15.1.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@testcontainers/postgresql": "^10.4.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.55.0",
    "supertest": "^7.2.2",
    "testcontainers": "^10.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.2",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
